# Configuração API Omie
OMIE_APP_KEY=seu_app_key_aqui
OMIE_APP_SECRET=seu_app_secret_aqui

# Configuração Google
GOOGLE_CREDENTIALS_FILE=caminho/para/credentials.json
GOOGLE_SPREADSHEET_KEY=sua_planilha_key_aqui
GOOGLE_MAPS_API_KEY=sua_google_maps_api_key

# Configuração Flask (opcional)
FLASK_SECRET_KEY=sua_chave_secreta_aqui

# Configuração ambiente
FLASK_ENV=development

# Configuração Banco de Dados
# Para PostgreSQL (Railway/Produção):
# DATABASE_URL=postgresql://usuario:senha@host:porta/database
# Para SQLite (Local/Desenvolvimento):
# DATABASE_URL=sqlite:///data/local_financeiro.db
DATABASE_URL=postgresql://seu_usuario:sua_senha@seu_host:porta/seu_database

# Outras configurações
CONFIDENCE_THRESHOLD=0.7
# UPLOAD_FOLDER=uploads/
